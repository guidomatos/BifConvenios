' These String Compare functions ignore trailing space and are case insensitive
Public Function StrEquals(val1 as Object, val2 as Object) As Boolean
	If IsNothing(val1) or IsNothing(val2) then Return False
	If val1.ToString().TrimEnd().ToUpper() = val2.ToString().TrimEnd().ToUpper() then Return True
	Return False
End Function

Public Function StrCompare(val1 as Object, val2 as Object) As Integer
	If IsNothing(val1) or IsNothing(val2) then Return -1
	Return String.Compare(val1.ToString().TrimEnd().ToUpper(), val2.ToString().TrimEnd().ToUpper())
End Function

Public Function CRFconvenioentidad(Fields As Microsoft.ReportingServices.ReportProcessing.ReportObjectModel.Fields) As String
	CRFconvenioentidad = "Le informamos que mantiene en nuestra Institución la(s) siguiente(s) obligación(es)  impaga(s), derivadas del préstamo personal otorgado en amparo del Convenio entre BIF y la empresa " &amp; Fields!DLNCC.Value &amp; " (empleador)."
End Function

Public Function CRFcuotapor(Fields As Microsoft.ReportingServices.ReportProcessing.ReportObjectModel.Fields) As String
	CRFcuotapor = "● Cuota por un monto a la fecha de " &amp; CRFmoneda(Fields) &amp; " " &amp; CStr(Round(Fields!DLEIC.Value * 1.0007, 2)) &amp; " (" &amp; CRFfechacuota(Fields) &amp; ")"
End Function

Public Function CRFfechacuota(Fields As Microsoft.ReportingServices.ReportProcessing.ReportObjectModel.Fields) As String
	CRFfechacuota = Format(DateSerial(2000 + Fields!DLVCA.Value, Fields!DLVCM.Value, Fields!DLVCD.Value), "dd/MM/yyyy")
End Function

Public Function CRFfechaimpresion(Fields As Microsoft.ReportingServices.ReportProcessing.ReportObjectModel.Fields) As String
	Dim mesSp as String
	Select Case Month(Today())
	Case 1
		mesSp = "Enero"
	Case 2
		mesSp = "Febrero"
	Case 3
		mesSp = "Marzo"
	Case 4
		mesSp = "Abril"
	Case 5
		mesSp = "Mayo"
	Case 6
		mesSp = "Junio"
	Case 7
		mesSp = "Julio"
	Case 8
		mesSp = "Agosto"
	Case 9
		mesSp = "Setiembre"
	Case 10
		mesSp = "Octubre"
	Case 11
		mesSp = "Noviembre"
	Case 12
		mesSp = "Diciembre"
	Case Else
		mesSp = ""
	End Select
	CRFfechaimpresion = StrConv(Fields!CIUDAG.Value, VbStrConv.ProperCase) &amp; ", " &amp; Format(Day(Today())) &amp; " de " &amp; mesSp &amp; " del " &amp; Format(Year(Today()))
End Function

Public Function CRFmensajeoficinatelefono() As String
	CRFmensajeoficinatelefono = "Finalmente, debemos señalar que los pagos correspondientes a la(s) obligación(es) requerida(s) en el primer párrafo, los puede realizar en cualquiera de las oficinas del BIF o en su defecto en la cuenta corriente en moneda nacional que tenemos en el Banco de la Nación cuyo número se detalla a continuación: 0000237787."
End Function

Public Function CRFmensajepagobanconacion() As String
	CRFmensajepagobanconacion = "En caso el pago se haya hecho en la cuenta que tenemos en el Banco de la Nación, deberá faxearnos el  comprobante de pago con sus nombres completos totalmente legibles al siguiente número: (074)232137 o escanearnos al correo electrónico jmino@bif.com.pe. En ambos casos con atención a la señorita Jessica Mino."
End Function

Public Function CRFmoneda(Fields As Microsoft.ReportingServices.ReportProcessing.ReportObjectModel.Fields) As String
	If UCase(Trim(Fields!DLCCY.Value)) = "SOL" Then
		Return "S/."
	End If
	If UCase(Trim(Fields!DLCCY.Value)) = "USD" Then
		Return "USD$"
	End If
	Return Fields!DLCCY.Value
End Function

Public Function CRFsenora(Fields As Microsoft.ReportingServices.ReportProcessing.ReportObjectModel.Fields) As String
	CRFsenora = Iif(UCase(RTrim(Fields!CUSSEX.Value)) = "F", "Señora:", "Señor:")
End Function

