    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSetCuotasPendientes</DataSourceName>
        <CommandText>
Select DatosDescuentoDetail.DLACC
	, DatosDescuentoDetail.DLVCA
	, DatosDescuentoDetail.DLVCM
	, DatosDescuentoDetail.DLVCD
	, DatosDescuentoDetail.DLCCY
	, DatosDescuentoDetail.DLEIC
	, DatosDescuentoHeader.DLACC as DatosDescuentoHeader_DLACC
	, DatosDescuentoHeader.CUSNA1
	, DatosDescuentoHeader.DIRECCION
	, DatosDescuentoHeader.CUSCTY
	, DatosDescuentoHeader.CUSZPC
	, DatosDescuentoHeader.DLNCC
	, DatosDescuentoHeader.CIUDAG
	, DatosDescuentoHeader.CUSSEX
	, DatosDescuentoHeader.SIGN1NAME
	, DatosDescuentoHeader.SIGN2NAME
	, DatosDescuentoHeader.SIGN1POSITION
	, DatosDescuentoHeader.SIGN2POSITION
	, DatosDescuentoHeader.SIGN1DATA
	, DatosDescuentoHeader.SIGN2DATA
 From DatosDescuentoDetail
 INNER JOIN DatosDescuentoHeader on (DatosDescuentoDetail.DLACC = DatosDescuentoHeader.DLACC)
 Where DatosDescuentoHeader.DLACC = DatosDescuentoDetail.DLACC
 Order by DatosDescuentoDetail.DLACC desc
</CommandText>
